# Copy to .env
LLM_PROVIDER=ollama          # openai | ollama
EMBEDDING_PROVIDER=ollama    # openai | ollama

# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBED_MODEL=text-embedding-3-small

# Ollama (local)
OLLAMA_BASE_URL=http://host.docker.internal:11434
OLLAMA_MODEL=llama3.1:8b
OLLAMA_EMBED_MODEL=nomic-embed-text

# Qdrant
QDRANT_URL=http://qdrant:6333
QDRANT_COLLECTION=kb_docs

# Flowise app-level auth (recommended)
FLOWISE_USERNAME=admin
FLOWISE_PASSWORD=change-me

# --- Gateway / Auth ---
JWT_SECRET=dev-secret-change-me
DEFAULT_RAG_ENGINE=llamaindex

# --- Observability (OpenTelemetry) ---
OTEL_ENABLED=true
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4318
ENVIRONMENT=local
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin1234
