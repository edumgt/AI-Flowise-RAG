# ==== COMMON ====
ENV=dev
JWT_SECRET=change-me-super-secret
CORS_ORIGIN=http://localhost:3005

# ==== LLM / Embeddings (choose one) ====
LLM_PROVIDER=ollama
OPENAI_API_KEY=YOUR_OPENAI_KEY
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDING_MODEL=text-embedding-3-large

# Ollama option
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_LLM_MODEL=llama3.1:8b
OLLAMA_EMBED_MODEL=nomic-embed-text

# ==== VECTOR DB ====
QDRANT_URL=http://qdrant:6333

# ==== FLOWISE ====
FLOWISE_USERNAME=admin
FLOWISE_PASSWORD=admin1234
FLOWISE_PORT=3000

# ==== OBJECT STORAGE (MinIO dev) ====
S3_ENABLED=true
S3_ENDPOINT=http://minio:9000
S3_ACCESS_KEY=minio
S3_SECRET_KEY=minio1234
S3_BUCKET=bankrag-docs
S3_REGION=ap-northeast-2
S3_FORCE_PATH_STYLE=true

# ==== QUEUE ====
REDIS_URL=redis://redis:6379

# ==== OBSERVABILITY ====
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4318
SERVICE_NAMESPACE=bankrag

# Flowise deployed chatflow endpoint (optional)
FLOWISE_CHATFLOW_URL=

# Ollama model pull at startup (comma-separated)
OLLAMA_PULL_MODELS=llama3.1:8b,nomic-embed-text
